            android:name="com.jqorz.common.broadcast.MyReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name="com.jqorz.common.rotate.RotateActivity1"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.jqorz.common.rotate.RotateActivity2"
