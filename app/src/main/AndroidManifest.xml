        <activity android:name="com.jqorz.common.webview.WebView2Activity" />
        <activity android:name="com.jqorz.common.webview.WebView3Activity" />

        <provider
            android:name="com.jqorz.common.content_provider.UserInfoProvider"
            android:authorities="com.datedu.launcher.provider"
            android:exported="true"
            android:readPermission="com.datedu.launcher.provider.READ"
            android:writePermission="com.datedu.launcher.provider.WRITE" />
